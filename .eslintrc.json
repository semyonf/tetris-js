{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "env": { "node": true },
  "rules": {
    "prefer-spread": 0,
    "prefer-rest-params": 0
  },
  "overrides": [
    {
      "files": ["**/*test.ts"],
      "env": { "jest": true }
    }
  ]
}
